rt;\r\n}\r\n\r\n.social-nav {\r\n  padding: 15px;\r\n  position: relative;\r\n  z-index: 5;\r\n}\r\n\r\n.menu__item {\r\n  width: 40px;\r\n  margin-bottom: 20px;\r\n  transition: 0.3s ease-in-out;\r\n\r\n  &:hover {\r\n    transform: scale(1.2);\r\n    filter: brightness(200%);\r\n  }\r\n}\r\n\r\n#home,#about {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n}\r\n\r\n#home {\r\n  \r\n\r\n  &::before {\r\n    content: '';\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    left: 0;\r\n    bottom: 0;\r\n    transition: background 0.8s ease;\r\n  \r\n    background-image: linear-gradient(\r\n      rgba(0, 0, 0, 0.2), \r\n      rgba(0, 0, 0, 0.2)\r\n    ), url('../img/4.jpg');\r\n    background-size: cover;\r\n    background-repeat: no-repeat;\r\n    background-position: center center;\r\n    animation: bgImage 30s forwards infinite ease-in-out;\r\n  }\r\n}\r\n\r\n.layer {\r\n  overflow: hidden;\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  \r\n}\r\n\r\n@keyframes bgImage {\r\n  from {\r\n    transform: scale(1);\r\n  }\r\n  to {\r\n    transform: scale(3);\r\n  }\r\n} \r\n\r\n#hexGrid {\r\n  padding-left: 0;\r\n  position: absolute;\r\n  top: 55%;\r\n  left: 50%;\r\n  transform: translate(-50%,-50%);\r\n  \r\n \r\n}\r\n\r\n.logo {\r\n  position: relative;\r\n  z-index: 5;\r\n  margin-left: 20px;\r\n  display: block;\r\n  width: 100px;\r\n  transition: 0.3s;\r\n}\r\n\r\n.logo:hover, .logo:focus {\r\n  filter: brightness(150%);\r\n}\r\n\r\n\r\n.cursor {\r\n  position: absolute;\r\n  width: 25px;\r\n  height: 25px;\r\n  border-radius: 50%;\r\n  border: 1px solid #fff;\r\n  z-index: 1;\r\n  pointer-events: none;\r\n  transform-origin: 100%, 100%;\r\n  transition: all 0.3s ease;\r\n  transition-property: background, transform;\r\n  transform: translate(-50%,-50%);\r\n}\r\n\r\n.link-grow {\r\n  transform: scale(2);\r\n  background:linear-gradient(#8a25b1,#16c0f0);\r\n  box-shadow: 0 0 10p